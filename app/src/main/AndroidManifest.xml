<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.passportautomation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PassportAutomation">
        <activity
            android:name=".police"

            android:label="Police"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".radmin"

            android:label="Regional Administrator"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ppadmin"

            android:label="Passport Administrator"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity3"

            android:label="Applicant Status"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity2"
            android:exported="false"
            android:label="Sign In as Applicant"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>